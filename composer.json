{
  "name": "andrewandante/silverstripe-staticpublishqueuesubsites",
  "type": "silverstripe-vendormodule",
  "description": "Adds configuration to connect subsites and staticpublishqueue.",
  "homepage": "https://github.com/andrewandante/silverstripe-staticpublishqueuesubsites",
  "keywords": ["staticpublishqueue", "silverstripe", "subsites", "static"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Andrew Aitken-Fincham",
      "role": "Developer",
      "email": "andrew@silverstripe.com",
      "homepage": "https://www.silverstripe.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "silverstripe/framework": "^4",
    "silverstripe/staticpublishqueue": "^3",
    "silverstripe/subsites": "^2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "squizlabs/php_codesniffer": "^3"
  },
  "autoload": {
    "psr-4": {
      "SilverStripe\\StaticPublishQueue\\Subsites": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
